{
  "bindings": [
    {
      "direction": "in",
      "type": "httpTrigger",
      "name": "req",
      "methods": ["get", "post"],
      "authLevel": "function"
    },
    {
      "direction": "out",
      "type": "http",
      "name": "$return"
    }
  ],
  "scriptFile": "function_app.py",
  "entryPoint": "main"
}
